{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngx-hal-forms": {
      "projectType": "library",
      "root": "projects/ngx-hal-forms",
      "sourceRoot": "projects/ngx-hal-forms/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ngx-hal-forms/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-hal-forms/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ngx-hal-forms/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/ngx-hal-forms/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "ngx-training": {
      "projectType": "library",
      "root": "projects/ngx-training",
      "sourceRoot": "projects/ngx-training/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ngx-training/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-training/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ngx-training/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/ngx-training/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "b7ad7405-fba0-4e43-9182-89a9348666a3"
  }
}
